<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ExpenseFilter">
    <Spinner
        android:id="@+id/spinner5"
        android:layout_width="130dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="74dp"
        android:layout_marginRight="74dp"
        android:layout_marginTop="38dp"
        android:background="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="90dp"
        android:layout_height="50dp"
        android:layout_alignTop="@+id/spinner5"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="15dp"
        android:layout_toLeftOf="@+id/spinner5"
        android:layout_toStartOf="@+id/spinner5"
        android:text="Type:"

        android:textColor="@android:color/black"
        android:textSize="32dp" />

    <TextView
        android:id="@+id/textView101"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/editText61"
        android:layout_marginEnd="22dp"
        android:layout_marginRight="22dp"
        android:layout_toLeftOf="@+id/editText71"
        android:layout_toStartOf="@+id/editText71"
        android:text="To:"
        android:textColor="@android:color/black"
        android:textSize="25dp" />

    <TextView
        android:id="@+id/textView111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/editText61"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:text="From:"
        android:textColor="@android:color/black"
        android:textSize="25dp" />

    <EditText
        android:id="@+id/editText61"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_below="@+id/spinner5"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="38dp"
        android:layout_toEndOf="@+id/textView111"
        android:layout_toRightOf="@+id/textView111"
        android:background="@android:color/darker_gray"
        android:ems="10"
        android:inputType="date"
        android:text="DD/MM/YY" />

    <EditText
        android:id="@+id/editText71"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/textView101"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp"
        android:background="@android:color/darker_gray"
        android:ems="10"
        android:inputType="date"
        android:text="DD/MM/YY" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView111"
        android:layout_alignStart="@+id/textView111"
        android:layout_below="@+id/editText61"
        android:layout_marginTop="33dp"
        android:text="TYPE"
        android:textStyle="bold"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="25dp" />

</RelativeLayout>